<div class="page-header">
	<div class="row">
		<div class="col-md-8">
			<h2>Readings</h2>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-md-8">
		<table class="table table-striped">
            <thead>
              <tr>
                <th>Glucose level</th>
                <th>Reading time</th>
              </tr>
            </thead>
            <tbody>
            	<% @readings.each do |reading| %>
					<tr>
						<td><%= reading.glucose_level %></td>
						<td><%= reading.created_at %></td>
					</tr>
              	<% end %>
            </tbody>
          </table>
	</div>
</div>